<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:id="@+id/r_jsf"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/c_ffffff"
    android:orientation="vertical"
    android:paddingBottom="12dp"
    android:paddingLeft="12dp"
    android:paddingTop="12dp">

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/sd_icon"
        android:layout_width="67dp"
        android:layout_height="67dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="9dp"
        fresco:actualImageScaleType="focusCrop"
        fresco:fadeDuration="3"
        fresco:placeholderImage="@color/c_f2"
        fresco:placeholderImageScaleType="focusCrop" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginRight="10dp"
        android:layout_toEndOf="@+id/sd_icon"
        android:layout_toRightOf="@+id/sd_icon"
        android:text="TextView"
        android:textColor="@color/c_33"
        android:textSize="15sp" />

    <LinearLayout
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/sd_icon"
        android:layout_toRightOf="@+id/sd_icon"
        android:orientation="horizontal"/>

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/sd_icon"
        android:layout_toRightOf="@+id/sd_icon"
        android:text="TextView"
        android:textColor="@color/c_33"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_is_gz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView21"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/textView38"
        android:layout_toRightOf="@+id/textView38"
        android:textColor="@color/fea21249"
        android:textSize="12sp" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/sd_to_jsf"
        android:layout_width="56dp"
        android:layout_height="64dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:padding="24dp"
        fresco:actualImageScaleType="focusCrop"
        fresco:fadeDuration="3"
        fresco:placeholderImage="@drawable/home_arow"
        fresco:placeholderImageScaleType="focusCrop" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView19"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="8dp"
        android:layout_toEndOf="@+id/textView21"
        android:layout_toLeftOf="@+id/sd_to_jsf"
        android:layout_toRightOf="@+id/textView21"
        android:layout_toStartOf="@+id/sd_to_jsf">

        <TextView
            android:id="@+id/textView40"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/sd_xb"
            android:layout_toStartOf="@+id/sd_xb"
            android:gravity="right"
            android:text="TextView"
            android:textColor="@color/c_33"
            android:textSize="15sp" />

        <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/sd_xb"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:layout_marginLeft="4dp"
            fresco:actualImageScaleType="focusCrop"
            fresco:fadeDuration="3"
            fresco:placeholderImageScaleType="focusCrop" />
    </RelativeLayout>
</RelativeLayout>