<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_item_list_city_jsf_list"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/c_ffffff"
    android:orientation="vertical"
    android:paddingBottom="10dp"
    android:paddingLeft="15dp"
    android:paddingTop="10dp">

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="10dp"
        android:text="TextView"
        android:textColor="@color/c_33"
        android:textSize="14sp"/>

    <LinearLayout
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"></LinearLayout>

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textColor="@color/c_33"
        android:textSize="12sp"/>

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView38"
        android:layout_marginTop="8dp"
        android:layout_toLeftOf="@+id/textView42"
        android:layout_toStartOf="@+id/textView42"
        android:text="TextView"
        android:textColor="@color/c_33"
        android:textSize="12sp"/>

    <TextView
        android:id="@+id/textView41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignTop="@+id/textView19"
        android:layout_toEndOf="@+id/textView19"
        android:layout_toRightOf="@+id/textView19"
        android:gravity="center"
        android:singleLine="true"
        android:text="TextView"
        android:textColor="@color/c_4491F2"
        android:textSize="12sp"/>

    <TextView
        android:id="@+id/textView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView39"
        android:layout_below="@+id/textView38"
        android:layout_marginLeft="4dp"
        android:layout_toLeftOf="@+id/sd_to_jsf"
        android:layout_toStartOf="@+id/sd_to_jsf"
        android:gravity="right|bottom"
        android:text="TextView"
        android:textColor="@color/fea21249"
        android:textSize="16sp"/>

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/sd_to_jsf"
        android:layout_width="56dp"
        android:layout_height="64dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:padding="24dp"
        fresco:actualImageScaleType="focusCrop"
        fresco:fadeDuration="3"
        fresco:placeholderImage="@drawable/home_arow"
        fresco:placeholderImageScaleType="focusCrop"
        fresco:pressedStateOverlayImage="@drawable/bg_toobar_btn"/>
</RelativeLayout>